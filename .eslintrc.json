{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "airbnb",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-var": 2,
		"indent": [2, "tab"],
		"no-tabs": 0,
		"prefer-arrow-callback": 1,
		"no-plusplus": 0,
		"no-duplicate-imports": 1,
		"no-underscore-dangle": 0,
		"operator-linebreak": 0,
		"object-curly-newline": 0
	}
}
