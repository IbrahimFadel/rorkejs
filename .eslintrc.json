{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "google"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-var": 2,
		"indent": [2, "tab"],
		"no-tabs": 0
	}
}
