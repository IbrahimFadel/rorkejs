{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"includePattern": ".+\\.js",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	}
}
